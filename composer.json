{
    "name": "leopoletto/robots-txt-parser",
    "description": "A comprehensive PHP package for parsing robots.txt files, including support for meta tags and X-Robots-Tag HTTP headers",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Poletto",
            "email": "hello@leopoletto.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.21.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes",
        "format:check": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes --dry-run --diff"
    },
    "autoload": {
        "psr-4": {
            "Leopoletto\\RobotsTxtParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leopoletto\\RobotsTxtParser\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}